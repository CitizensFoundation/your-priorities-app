    "concurrently": "^7.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "fetch-mock": "^9.11.0",
    "koa-proxies": "^0.12.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "sinon": "^14.0.0",
    "ts-lit-plugin": "^1.2.1",
    "tslib": "^2.4.0",
    "typescript": "~4.7.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@open-wc",
      "prettier"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "overrides": [
        {
          "rules": {
            "no-unused-expressions": "off",
            "no-unused-vars": "off",
            "eslint-disable wc/guard-super-call": "off",
            "eslint-disable no-nested-ternary": "off",
            "eslint-disable no-else-return": "off",
            "eslint-disable no-plusplus": "off",
            "eslint-disable import/no-duplicates": "off",
            "eslint-disable import/order": "off",
            "eslint-disable class-methods-use-this": "off"
          }
        }
      ],
      "no-unused-vars": "off",
      "eslint-disable wc/guard-super-call": "off",
      "eslint-disable no-nested-ternary": "off",
      "eslint-disable no-else-return": "off",
      "eslint-disable no-plusplus": "off",
      "eslint-disable import/no-duplicates": "off",
      "eslint-disable import/order": "off",
      "eslint-disable class-methods-use-this": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "import/no-unresolved": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "ignorePackages": true
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
