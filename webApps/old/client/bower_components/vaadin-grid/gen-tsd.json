{
  "excludeFiles": [
    "wct.conf.js",
    "index.html",
    "all-imports.js",
    "src/all-imports.js",
    "src/iron-list.js",
    "src/vaadin-grid-styles.js",
    "src/vaadin-grid-templatizer.js",
    "src/vaadin-grid-helpers.js",
    "demo/**/*",
    "test/**/*",
    "theme/**/*"
  ],
  "autoImport": {
    "./src/vaadin-grid.js": [
      "GridElement"
    ],
    "./src/vaadin-grid-column.js": [
      "GridColumnElement"
    ],
    "./src/vaadin-grid-column-group.js": [
      "GridColumnGroupElement"
    ],
    "./@types/interfaces": [
      "GridBodyRenderer",
      "GridCellClassNameGenerator",
      "GridColumnTextAlign",
      "GridDataProviderCallback",
      "GridDataProviderParams",
      "GridDataProvider",
      "GridDragAndDropFilter",
      "GridDropMode",
      "GridEventContext",
      "GridFilter",
      "GridHeaderFooterRenderer",
      "GridItem",
      "GridItemModel",
      "GridRowDetailsRenderer",
      "GridSorterDirection",
      "GridSorter"
    ]
  }
}
