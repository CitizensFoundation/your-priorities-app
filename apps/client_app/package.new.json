{
  "scripts": {
    "lint:eslint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.js\" \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "tsc && web-test-runner --coverage --node-resolve",
    "test:watch": "web-test-runner --watch --node-resolve",
    "storybook": "concurrently --kill-others --names tsc,storybook \"npm run tsc:watch\" \"start-storybook --node-resolve --watch --open\"",
    "storybook:build": "build-storybook",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js",
    "start:build": "npm run build && web-dev-server --root-dir dist --app-index index.html --open --compatibility none",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.10.0",
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^3.0.0-next.2",
    "@open-wc/testing-helpers": "^2.0.0-next.0",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@types/google.analytics": "0.0.41",
    "@types/i18next": "^13.0.0",
    "@types/insert-text-at-cursor": "^0.3.0",
    "@types/linkifyjs": "^2.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "15.12.2",
    "@types/recordrtc": "^5.6.5",
    "@types/wavesurfer.js": "^5.0.1",
    "@types/webappsec-credential-management": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@web/dev-server": "0.1.17",
    "@web/dev-server-rollup": "^0.3.4",
    "@web/test-runner": "^0.13.11",
    "chai": "^4.3.4",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "fetch-mock": "^9.11.0",
    "koa-proxies": "^0.12.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-workbox": "^6.1.1",
    "sinon": "^10.0.0",
    "ts-lit-plugin": "^1.2.1",
    "tslib": "^2.3.0",
    "typescript": "~4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "name": "yp-app",
  "version": "9.0.0",
  "description": "Webcomponent yp-app",
  "author": "yp-app",
  "license": "LGPL",
  "dependencies": {
    "@joeattardi/emoji-button": "^4.6.0",
